<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GaleriaDeRecursos2.AppFlyout"
            xmlns:pages="clr-namespace:GaleriaDeRecursos2.Views"
            FlyoutLayoutBehavior="Popover"
             Title="AppFlyout">

    <FlyoutPage.Flyout>
        <pages:Menu/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:Principal/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>